<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context="com.guestlogix.traveleruikit.activities.OrdersActivity"
        android:id="@+id/ordersContainerFrameLayout"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent">
    <com.google.android.material.tabs.TabLayout
            android:id="@+id/tab_OrdersActivity_ordersTabs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    <androidx.viewpager.widget.ViewPager
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/viewPager_OrdersActivity_ordersPager"/>
</LinearLayout>