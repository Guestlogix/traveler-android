<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".fragments.FlightSearchResultsFragment"
        tools:listitem="@layout/fragment_flight_search_results_item"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/flightResultRecyclerView"
            android:name="com.guestlogix.traveler.activities.FlightSearchResultsFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:background="#EFEFEF"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
    />

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            tools:layout_editor_absoluteY="231dp"
            tools:layout_editor_absoluteX="148dp"
            android:text="@string/oh_no" tools:ignore="MissingConstraints" android:id="@+id/testId"/>
</androidx.constraintlayout.widget.ConstraintLayout>